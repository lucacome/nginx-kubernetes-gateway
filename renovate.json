{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>nginxinc/k8s-common"
  ],
  "kubernetes": {
    "fileMatch": [
      "examples/.+\\.yaml$",
      "suite/.+\\.yaml$"
    ]
  },
  "packageRules": [
    {
      "matchDatasources": [
        "golang-version"
      ],
      "rangeStrategy": "bump"
    }
  ]
}
